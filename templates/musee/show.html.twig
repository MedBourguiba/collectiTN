
{% extends 'backAdmin.html.twig' %}
{% block title %}Show Musee{% endblock %}

{% block body %}
    <h1>Musee</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Name</th>
                <td>{{ musee.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ musee.description }}</td>
            </tr>
            <tr>
                <th>Img</th>
                <td>{{ musee.img }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ musee.createdAt ? musee.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_musee_index') }}">back to list</a>

    <a href="{{ path('app_musee_edit', {'id': musee.id}) }}">edit</a>

    {{ include('musee/_delete_form.html.twig') }}
{% endblock %}
